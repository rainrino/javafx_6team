<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jungeun.controller.MainController">
   <children>
      <ImageView fx:id="logoImg" fitHeight="60.0" fitWidth="72.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets bottom="22.0" left="264.0" right="264.0" top="22.0" />
         </VBox.margin>
         <image>
            <Image url="@../../img/logo.png" />
         </image>
      </ImageView>
      <HBox prefHeight="40.0" prefWidth="600.0">
         <children>
            <Button fx:id="homeBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="40.0" prefWidth="300.0" style="-fx-background-color: rgb(52,152,219);" text="홈" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="mypageBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="40.0" prefWidth="300.0" style="-fx-background-color: rgb(224, 224, 224);" text="마이 페이지" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <Label text="최근에 올라온 심리테스트">
         <font>
            <Font size="24.0" />
         </font>
         <VBox.margin>
            <Insets left="30.0" top="30.0" />
         </VBox.margin>
      </Label>
      <HBox prefHeight="240.0" prefWidth="600.0" spacing="30.0">
         <children>
            <Pane prefHeight="240.0" prefWidth="160.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <ImageView fx:id="test1Img" fitHeight="220.0" fitWidth="160.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../img/test1_icon.png" />
                           </image>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </ImageView>
                        <Label fx:id="test1Lbl" alignment="CENTER" prefHeight="20.0" prefWidth="160.0" text="다이어트 의지 테스트" textAlignment="CENTER">
                           <font>
                              <Font size="14.0" />
                           </font>
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                  </VBox>
                  <Button fx:id="test1Btn" mnemonicParsing="false" onAction="#OpenTest1Page" opacity="0.0" prefHeight="240.0" prefWidth="160.0" />
               </children>
            </Pane>
            <Pane prefHeight="240.0" prefWidth="160.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <ImageView fx:id="test2Img" fitHeight="220.0" fitWidth="160.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../img/test2_icon.png" />
                           </image>
                        </ImageView>
                        <Label fx:id="test2Lbl" alignment="CENTER" prefHeight="20.0" prefWidth="160.0" text="소비성향 테스트" textAlignment="CENTER">
                           <font>
                              <Font size="14.0" />
                           </font>
                           <VBox.margin>
                              <Insets top="15.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                  </VBox>
                  <Button fx:id="test2Btn" mnemonicParsing="false" onAction="#OpenTest2Page" opacity="0.0" prefHeight="240.0" prefWidth="160.0" />
               </children>
            </Pane>
            <Pane prefHeight="240.0" prefWidth="160.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <ImageView fx:id="test3Img" fitHeight="220.0" fitWidth="160.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../img/test3_icon.png" />
                           </image>
                        </ImageView>
                        <Label fx:id="test3Lbl" alignment="CENTER" prefHeight="20.0" prefWidth="160.0" text="모임성향 테스트" textAlignment="CENTER">
                           <font>
                              <Font size="14.0" />
                           </font>
                           <VBox.margin>
                              <Insets top="8.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                  </VBox>
                  <Button fx:id="test3Btn" mnemonicParsing="false" onAction="#OpenTest3Page" opacity="0.0" prefHeight="240.0" prefWidth="160.0" />
               </children>
            </Pane>
         </children>
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
         <padding>
            <Insets left="30.0" />
         </padding>
      </HBox>
   </children>
</VBox>
