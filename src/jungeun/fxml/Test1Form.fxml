<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="">
   <children>
      <ImageView fx:id="logoImg" fitHeight="60.0" fitWidth="72.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets bottom="22.0" left="264.0" right="264.0" top="22.0" />
         </VBox.margin>
      </ImageView>
      <HBox prefHeight="40.0" prefWidth="600.0">
         <children>
            <Button fx:id="homeBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="40.0" prefWidth="300.0" style="-fx-background-color: rgb(52,152,219);" text="홈" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="mypageBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="40.0" prefWidth="300.0" style="-fx-background-color: rgb(224, 224, 224);" text="마이 페이지" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <ImageView fx:id="test1Img" fitHeight="180.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets bottom="20.0" left="40.0" right="20.0" top="20.0" />
               </HBox.margin>
            </ImageView>
            <VBox prefHeight="220.0" prefWidth="400.0">
               <children>
                  <Label text="다이어트 의지 테스트">
                     <font>
                        <Font size="36.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="40.0" />
                     </VBox.margin>
                  </Label>
                  <Label alignment="TOP_LEFT" prefHeight="120.0" prefWidth="400.0" text="초록식탁 제공 다음줄 어떻게가지 0회 조회">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
         </children>
      </HBox>
      <Label alignment="TOP_LEFT" prefHeight="130.0" prefWidth="500.0" text="다이어트 하고싶은 당신!">
         <VBox.margin>
            <Insets left="50.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="testBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="525.0" style="-fx-background-color: rgb(52,152,219);" text="테스트 시작" textFill="WHITE">
         <VBox.margin>
            <Insets left="40.0" top="40.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
